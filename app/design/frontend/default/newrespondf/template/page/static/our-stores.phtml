        jQuery(".account-nav .flex-control-nav.flex-control-paging li a").attr("href","#");


        jQuery(".l-align li").on("click tap", "a", function (e) {
            e.preventDefault();
            jQuery(".ys_static_page_dropdown li a").removeClass('active_link');
            jQuery(this).addClass('active_link');
